<!DOCTYPE html>
<html>
    <head>
        <style>
        
        </style>
    </head>
    <body>
        <img width="150" src="https://point.qreport.site/files/logopiccargo.svg" alt="">
        <div style="width: 95%; margin: 0 1.5%;">
            <div style="text-align: center; padding-left: 0px;">            
                <h3>CONTROL DE FILE</h5>
                <table style="text-align: left; font-size: 11px; " class="tableMain">
                    <tr class="encMain">
                        <th width="15%">EXP</th>
                        <th width="10%">POL/POD</th>
                        <th width="25%">GENERALES <br>  EMBARQUES</th>
                        <th width="20%">SERVICIOS</th>
                        <th width="20%">FECHAS</th>
                        <th width="10%">OPERADOR</th>
                    </tr>
                <% if (items.length > 0) { %>
                    <% items.forEach(item => {%>
                       <tr 
                       ><td>
                           <p>E.M.:<%= item.dataHouse.code_master %></p>
                           <p>E.H.:<%= item.dataHouse.code_house %></p>
                           <p>v.: Sin Vendedor</p>
                       </td>
                       <td>
                           <p><%= item.dataHouse.nameModality %> <%= item.dataHouse.nameShipment %></p>
                           <p><%= item.dataHouse.namePortBegin %></p>
                           <p><%= item.dataHouse.namePortEnd %></p>
                       </td>
                       <td>
                           <p>A.:<%= item.dataHouse.nameAgent %></p>
                           <p>CSG.:<%= item.dataHouse.nameConsigner %></p>
                           <p>N.:<%= item.dataHouse.nameNotify %></p>
                       </td>
                       <td>
                            
                            <% item.dataService[0].forEach(ser => {%>
                                    <p><%= ser.nameService %>:
                                        <% if (ser.status = 0) { %>
                                        NO
                                        <% }%>
                                         <% if (ser.status = 1) { %>
                                        SI
                                        <% }%>
                                    </p>
                                     
                                <%  });%>
       
                       </td>
                       <td>
                           <p>Salida: <%= item.dataHouse.fecha_etd %></p>
                           <p>Llegada: <%= item.dataHouse.fecha_eta %></p>
                           <p>Disponibilidad: <%= item.dataHouse.fecha_disponibilidad %></p>
                           <p>Recogida: <%= item.dataHouse.fecha_recogida %></p>
                           <p>CUTT-OFF-FIS: <%= item.dataHouse.fecha_c_f %></p>
                           <p>CUTT-OFF-DOC: <%= item.dataHouse.fecha_c_d %></p>
                       </td>
                       <td>
                           <p>Operador: <%= item.dataHouse.nameOperador %></p>
                           <p>Creado: <%= item.dataHouse.created %></p>
                       </td>
                       
                    </tr>
                    <%  });%>
                <% }%>
                </table>
                <table style="text-align: left; font-size: 11px; background-color: rgb(230, 230, 230)" width="100%" class="table_fott">
                    <tr>
                        <td>
                            <p>Total de Expedientes Master: <%= totalExpedientesMaster %></p>
                        </td>
                        <td>
                            <p>Total Importación Aérea: <%= totalIA %></p>
                            <p>Total Exportación Aérea: <%= totalEA %></p>
                            <p>Total Importación Marítima: <%= totalIM %></p>
                            <p>Total Exportación Marítima: <%= totalEM %></p>
                        </td>
                        <td>
                            <p>Total Llegados: <%= totalLL %></p>
                            <p>Total por Llegar: <%= totalPL %></p>
                            <p>Total Salidos: <%= totalSS %></p>
                            <p>Total por Salir: <%= totalPS %></p>
                        </td>
                    </tr>
                </table>
             </div>
        </div>
    </body>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap');
        *{
            font-family: 'Roboto', sans-serif;
        }
        .tableMain p{
            padding: 0px;
            margin: 0px;
        }

        .tableMain td{
            padding: 5px;
        }

        .tableMain .encMain{
            text-align: center !important;
            background-color: rgb(226, 226, 226);
        }

        .tableMain {
            border-collapse: collapse;
        }
        .tableMain, th, td {
            border-bottom: 1px solid rgb(207, 207, 207);
        }

        .table_fott {
            border-collapse: collapse;
        }
        /* .table_fott, th, td {
            border: 1px solid black;
        } */
    </style>
</html>