            DATOS DE MOVIMIENTO DE MERCANCIA
          </td>
        </tr>
        <tr>
          <td>PROVEEDOR:</td>
          <td><%=proveedor%></td>
        </tr>
        <tr>
          <td>POL:</td>
          <td><%=origen%></td>
        </tr>
        <tr>
          <td>POD:</td>
          <td><%=destino%></td>
        </tr>
      </tbody>
    </table>

    <table width="100%">
      <thead>
        <tr>
          <td colspan="2" class="center bold st-bg">SERVICIOS A REALIZAR</td>
        </tr>
      </thead>
      <tbody>
        <% if (listServiciosInstructivo.length>0) {%> <%
        listServiciosInstructivo.forEach(element => { %>
        <tr>
          <td><%=element.name %></td>
          <td><%=element.status %></td>
        </tr>
        <%}); %> <%}%>
      </tbody>
    </table>
    <br />
    <div style="page-break-after: always"></div> 

    <table width="100%" class="mi-tabla">
      <thead>
        <tr>
          <td colspan="5" class="center bold st-bg">INGRESOS</td>
        </tr>
      </thead>
      <tbody>
        <% if (listIngresosInstructivo.length>0) {%> <%
        listIngresosInstructivo.forEach((element,index) => { %>
        <tr>
          <% if (index === 0 || element.descripcion !== listIngresosInstructivo[
          index - 1 ].descripcion ) {%>

          <td
            width="35%"
            rowspan="<%=
                        listIngresosInstructivo.filter(
                          (v) => v.descripcion == element.descripcion
                        ).length %>"
          >
            <%= element.descripcion %>
          </td>

          <%}%>
          <td width="35%"><%= element.service %></td>
          <td width="10%" class="align-right"><%= element.valor %></td>
          <td width="10%" class="align-right"><%= element.igv %></td>
          <td width="10%" class="align-right"><%= element.total %></td>
        </tr>
        <%}); %> <%}%>
      </tbody>
    </table>
    <br />
    <table width="100%" class="mi-tabla">
      <thead>
        <tr>
          <td colspan="5" class="center bold st-bg">COSTOS</td>
        </tr>
      </thead>
      <tbody>
        <% if (listCostosInstructivo.length>0) {%> <%
        listCostosInstructivo.forEach((element,index) => { %>
        <tr>
          <% if (index === 0 || element.proveedor !== listCostosInstructivo[
          index - 1 ].proveedor ) {%>

          <td
            width="35%"
            rowspan="<%=
                        listCostosInstructivo.filter(
                          (v) => v.proveedor == element.proveedor
                        ).length %>"
          >
            <%= element.proveedor %>
          </td>

          <%}%>
          <td width="35%" class="align-right"><%= element.service %></td>
          <td width="10%" class="align-right"><%= element.valor %></td>
          <td width="10%" class="align-right"><%= element.igv %></td>
          <td width="10%" class="align-right"><%= element.total %></td>
        </tr>
        <%}); %> <%}%>
      </tbody>
    </table>
  </body>
</html>
